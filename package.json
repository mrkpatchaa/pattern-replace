{
  "name": "pattern-replace",
  "version": "1.0.0",
  "description": "Keycloak Configuration file duplicator",
  "main": "src/main.js",
  "bin": {
    "pattern-replace": "bin/index.js"
  },
  "scripts": {
    "build": "rimraf dist && webpack --config webpack/prod.config.js",
    "dev": "webpack-dev-server --config webpack/dev.config.js --progress --hot",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "jest -u --watch",
    "coverage": "jest -u --coverage"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/plugin-transform-spread": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "autoprefixer": "^10.4.14",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^29.5.0",
    "babel-loader": "9.1.2",
    "bootstrap": "^4.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "nodemon": "^2.0.22",
    "postcss-loader": "^7.3.3",
    "precss": "^3.1.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "babel-node"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.scss$": "<rootDir>/jestCssTransform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmkpatchaa/pattern-replace.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rmkpatchaa/pattern-replace/issues"
  },
  "homepage": "https://github.com/rmkpatchaa/pattern-replace#readme"
}
